# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# N8N Webhook Configuration
N8N_WEBHOOK_URL=http://localhost:5678/webhook/discord-event
# Hoặc nếu chạy trong Docker: http://n8n:5678/webhook/discord-event

# Bot Settings
BOT_PREFIX=!
ENABLE_MESSAGE_LOGGING=true
ENABLE_MEMBER_JOIN_LOGGING=true

# Optional: Guild ID để giới hạn bot chỉ hoạt động trong server cụ thể
# GUILD_ID=your_discord_server_id

# Logging Level (error, warn, info, debug)
LOG_LEVEL=info 